<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="Index" class="site_title"><i class="fa fa-paw"></i> <span>Home</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <!-- menu profile quick info -->
                    <div class="profile clearfix">
                        <div class="profile_pic">

                        </div>
                        <div class="profile_info">
                            <span>Welcome,</span>
                            <h2>@ViewBag.Name</h2>
                        </div>
                    </div>
                    <!-- /menu profile quick info -->
                    <br />
                    <!-- sidebar menu -->
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <h3>General</h3>
                            @if (ViewBag.IsAdmin == "Admin")
                            {
                                <ul class="nav side-menu">
                                    <li>
                                        <a><i class="fa fa-home"></i> Quản lý <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Chiến dịch", "QLChiendich", "Admin")</li>
                                            <li>@Html.ActionLink("Công việc", "QLCongviec", "Admin")</li>
                                            <li>@Html.ActionLink("Giảng viên", "QLGiangvien", "Admin")</li>
                                            <li>@Html.ActionLink("Sinh viên", "QLSinhvien", "Admin")</li>

                                        </ul>
                                    </li>
                                    <li>
                                        <a><i class="fa fa-edit"></i> Tìm kiếm <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Chiến dịch", "DSChiendich", "Admin")</li>
                                            <li>@Html.ActionLink("Công việc", "DSCongViec", "Admin")</li>
                                            <li>@Html.ActionLink("Giảng viên", "DSGiangvien", "Admin")</li>
                                            <li>@Html.ActionLink("Sinh viên", "DSSinhvien", "Admin")</li>
                                            <li>@Html.ActionLink("Chi phí", "DSChiphi", "Admin")</li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a><i class="fa fa-cog"></i> Nghiệp vụ <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Phân chia công việc", "Phanchiacongviec", "Admin")</li>
                                            <li>@Html.ActionLink("Tính chi phí", "TinhChiPhi", "Admin")</li>
                                            <li>@Html.ActionLink("Đánh giá công việc", "Danhgiacongviec", "Admin")</li>
                                        </ul>
                                    </li>
                                </ul>
                            }
                            else if (ViewBag.IsAdmin == "QTSV")
                            {
                                <ul class="nav side-menu">
                                    <li>
                                        <a><i class="fa fa-home"></i> Quản lý <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Sinh viên", "QLSinhvien", "Admin")</li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a><i class="fa fa-edit"></i> Tìm kiếm <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Sinh viên", "DSSinhvien", "Admin")</li>
                                        </ul>
                                    </li>
                                </ul>
                            }
                            else
                            {
                                <ul class="nav side-menu">
                                    <li>
                                        <a><i class="fa fa-home"></i> Quản lý <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Giảng viên", "QLGiangvien", "Admin")</li>
                                        </ul>
                                    </li>
                                    <li>
                                        <a><i class="fa fa-edit"></i> Tìm kiếm <span class="fa fa-chevron-down"></span></a>
                                        <ul class="nav child_menu">
                                            <li>@Html.ActionLink("Giảng viên", "DSGiangvien", "Admin")</li>
                                        </ul>
                                    </li>
                                </ul>
                            }


                        </div>
                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">
                        <a data-toggle="tooltip" data-placement="top" title="Settings">
                            <span class="glyphicon glyphicon-cog" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="FullScreen">
                            <span class="glyphicon glyphicon-fullscreen" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Lock">
                            <span class="glyphicon glyphicon-eye-close" aria-hidden="true"></span>
                        </a>
                        <a data-toggle="tooltip" data-placement="top" title="Logout" href="@Url.Action("Login", "Admin")">
                            <span class="glyphicon glyphicon-off" aria-hidden="true"></span>
                       
                        </a>
                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        @if (ViewBag.IsAdmin == "Admin")
                        {
                            <ul class="nav navbar-nav navbar-right">
                                <li class="">
                                    <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <img src="images/img.jpg" alt="">@ViewBag.Name
                                        <span class=" fa fa-angle-down"></span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                                        <li><a href="@Url.Action("QLNguoidung", "Admin")">Quản lý nhân viên</a></li>
                                        <li><a href="@Url.Action("QLChucvu", "Admin")">Quản lý chức vụ</a></li>
                                        <li><a href="@Url.Action("Login", "Admin")"><i class="fa fa-sign-out pull-right"></i> Đăng xuất</a></li>
                                    </ul>
                                </li>
                            </ul>
                        }
                        else
                        {
                            <ul class="nav navbar-nav navbar-right">
                                <li class="">
                                    <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                        <img src="images/img.jpg" alt="">@ViewBag.Name
                                        <span class=" fa fa-angle-down"></span>
                                    </a>
                                    <ul class="dropdown-menu dropdown-usermenu pull-right">
                                        <li><a href="@Url.Action("Suathongtincanhan", "Admin", new { name = ViewBag.Name })">Thay đổi thông tin</a></li>
                                        <li><a href="@Url.Action("Login", "Admin")"><i class="fa fa-sign-out pull-right"></i> Đăng xuất</a></li>
                                    </ul>
                                </li>
                            </ul>
                        }
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                    Gentelella - Bootstrap Admin Template by <a href="https://colorlib.com">Colorlib</a>
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>
    <!-- jQuery -->
    <script src="~/Templates/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/Templates/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <!-- FastClick -->
    <script src="~/Templates/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Templates/vendors/nprogress/nprogress.js"></script>
    <!-- Chart.js -->
    <script src="~/Templates/vendors/Chart.js/dist/Chart.min.js"></script>
    <!-- gauge.js -->
    <script src="~/Templates/vendors/gauge.js/dist/gauge.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Templates/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- iCheck -->
    <script src="~/Templates/vendors/iCheck/icheck.min.js"></script>
    <!-- Skycons -->
    <script src="~/Templates/vendors/skycons/skycons.js"></script>
    <!-- Flot -->
    <script src="~/Templates/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Templates/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Templates/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Templates/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Templates/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Templates/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Templates/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Templates/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- DateJS -->
    <script src="~/Templates/vendors/DateJS/build/date.js"></script>
    <!-- JQVMap -->
    <script src="~/Templates/vendors/jqvmap/dist/jquery.vmap.js"></script>
    <script src="~/Templates/vendors/jqvmap/dist/maps/jquery.vmap.world.js"></script>
    <script src="~/Templates/vendors/jqvmap/examples/js/jquery.vmap.sampledata.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Templates/vendors/moment/min/moment.min.js"></script>
    <script src="~/Templates/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- bootstrap-datetimepicker -->
    <script src="~/Templates/vendors/bootstrap-datetimepicker/build/js/bootstrap-datetimepicker.min.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/Templates/build/js/custom.min.js"></script>
    <!-- Parsley -->
    <script src="~/Templates/vendors/parsleyjs/dist/parsley.min.js"></script>
    <!-- Datatables -->
    <script src="~/Templates/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-buttons-bs/js/buttons.bootstrap.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-buttons/js/buttons.flash.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-buttons/js/buttons.html5.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-buttons/js/buttons.print.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-fixedheader/js/dataTables.fixedHeader.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-keytable/js/dataTables.keyTable.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Templates/vendors/datatables.net-responsive-bs/js/responsive.bootstrap.js"></script>
    <script src="~/Templates/vendors/datatables.net-scroller/js/dataTables.scroller.min.js"></script>
    <script src="~/Templates/vendors/jszip/dist/jszip.min.js"></script>
    <script src="~/Templates/vendors/pdfmake/build/pdfmake.min.js"></script>
    <script src="~/Templates/vendors/pdfmake/build/vfs_fonts.js"></script>
    <!-- jQuery Smart Wizard -->
    <script src="~/Templates/vendors/jQuery-Smart-Wizard/js/jquery.smartWizard.js"></script>
    <!-- jquery.inputmask -->
    <script src="~/Templates/vendors/jquery.inputmask/dist/min/jquery.inputmask.bundle.min.js"></script>
    <!-- Dropzone.js -->
