    <script>
        $('#myDatepicker').datetimepicker();

        $('#myDatepicker2').datetimepicker({
            format: 'DD.MM.YYYY'
        });
        $('#myDatepicker5').datetimepicker({
            format: 'MM/DD/YYYY'
        });
        $('#myDatepicker6').datetimepicker({
            format: 'MM/DD/YYYY'
        });
        $('#myDatepicker3').datetimepicker({
            format: 'hh:mm A'
        });

        $('#myDatepicker4').datetimepicker({
            ignoreReadonly: true,
            allowInputToggle: true
        });

        $('#datetimepicker6').datetimepicker();

        $('#datetimepicker7').datetimepicker({
            useCurrent: false
        });

        $("#datetimepicker6").on("dp.change", function (e) {
            $('#datetimepicker7').data("DateTimePicker").minDate(e.date);
        });

        $("#datetimepicker7").on("dp.change", function (e) {
            $('#datetimepicker6').data("DateTimePicker").maxDate(e.date);
        });



        $("#DSChiendich").DataTable();
        $("#DSCongviec").DataTable();
        $("#DSNguoithamgia").DataTable();
        $("#DSGiangvien").DataTable();
        $("#DSSinhvien").DataTable();
        $("#DSNhanvien").DataTable();
        $("#DSDoi").DataTable();
        $("#DSChucvu").DataTable();
        $("#DSKetquaCV").DataTable();


        // DS CHIEN DICH
        $(document).ready(function () {
            var table = $('#DSChiendich').DataTable();

            $('#DSChiendich tbody').on('click', 'tr', function () {
                var data = table.row(this).data();

                $('#campaignIDHidden').val(data[0]);
                $('#campaignID').val(data[0]);
                $('#campaignName').val(data[1]);
                var startday = new Date(data[2]);
                var time = parseInt(data[3].split("Ngày"));
                var endday = new Date(startday);
                endday.setDate(endday.getDate() + time);
                var strStartday = startday.getMonth() + 1 + '/' + startday.getDate() + '/' + startday.getFullYear();
                var strEndday = endday.getMonth() + 1 + '/' + endday.getDate() + '/' + endday.getFullYear();
                var uptime = strStartday + ' - ' + strEndday;
                $('#reservation').val(uptime);
                $('#campaignMeaning').val(data[4]);
                $('#campaignPneeded').val(parseInt(data[5]));
                $('#campaignPjoined').val(parseInt(data[6]));
                $('#campaignMoreinfo').val(data[8]);


            });
        });

        // DS CONG VIEC
        $(document).ready(function () {
            var table = $('#DSCongviec').DataTable();

            $('#DSCongviec tbody').on('click', 'tr', function () {
                var data = table.row(this).data();
                $('#campaignID').val(data[0]);
                $('#jobIDHidden').val(data[1]);
                $('#jobID').val(data[1]);
                $('#jobName').val(data[2]);
                $('#jobPlace').val(data[3]);
                $('#jobDetails').val(data[4]);
                $('#jobPneeded').val(parseInt(data[5]));
                var startday = new Date(data[6]);
                var time = parseInt(data[7].split("Ngày"));
                var endday = new Date(startday);
                endday.setDate(endday.getDate() + time); var strStartday = startday.getMonth() + 1 + '/' + startday.getDate() + '/' + startday.getFullYear();
                var strEndday = endday.getMonth() + 1 + '/' + endday.getDate() + '/' + endday.getFullYear();
                var uptime = strStartday + ' - ' + strEndday;
                $('#reservation').val(uptime);
                $('#jobCost').val(parseFloat(data[8]));
                $('#jobMoreinfo').val(data[9]);
            });
        });

        // DS NGUOI THAM GIA
        $(document).ready(function () {
            var table = $('#DSNguoithamgia').DataTable();

            $('#DSNguoithamgia tbody').on('click', 'tr', function () {
                var data = table.row(this).data();

                $('#participaterID').val(data[1]);
                $('#participaterName').val(data[2]);
                $('#positionSelect').val(data[3]);
                $('#paticipaterPosition').val(data[3]);
                var birthday = new Date(data[4]);
                var partyjoinday = new Date(data[8]);

                var strBirthday = birthday.getMonth() + '/' + birthday.getDate() + '/' + birthday.getFullYear();
                var strPartyjoinday = partyjoinday.getMonth() + '/' + partyjoinday.getDate() + '/' + partyjoinday.getFullYear();

                $('#participaterBirthday').val(strBirthday);
                $('#gender').val(data[5]);
                $('#participaterPhone').val(data[6]);
                $('#studentClass').val(data[7]);
                $('#studentPartyJoinDay').val(strPartyjoinday);
                $('#studentMark').val(parseFloat(data[9]));
                $('#facultySelect').val(data[10]);



            });
        });

        // DS DOI
        $(document).ready(function () {
            var table = $('#DSDoi').DataTable();

            $('#DSDoi tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#teamIDHidden').val(data[1]);
                $('#teamID').val(data[1]);

                $('#teamID').val(data[1]);
                $('#participaterjoin').val(parseInt(data[2].split('/')[0]));
                $('#participaterneed').val(parseInt(data[2].split('/')[1]));


            });
        });

        // DS CHI PHI PHAT SINH
        $(document).ready(function () {
            var table = $('#DSChiphiphatsinh').DataTable();

            $('#DSChiphiphatsinh tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#jobID').val(data[0]);
                $('#feeID').val(data[1]);
                $('#feeName').val(data[2]);
                $('#feeMeaning').val(data[3]);
                $('#feeCertificate').val(data[4]);
                $('#feePrice').val(data[5]);
            });
        });

        // DS GIANG VIEN
        $(document).ready(function () {
            var table = $('#DSGiangvien').DataTable();

            $('#DSGiangvien tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#campaignID').val(data[0]);
                $('#participaterID').val(data[1]);
                $('#participaterName').val(data[2]);

                var birthday = new Date(data[3]);
                var strBirthday = birthday.getMonth() + '/' + birthday.getDate() + '/' + birthday.getFullYear();
                $('#participaterBirthday').val(strBirthday);

                $('#gender').val(data[4]);
                $('#participaterPhone').val(data[5]);
                $('#facultySelect').val(data[6]);
            });
        });

        // DS SINH VIEN
        $(document).ready(function () {
            var table = $('#DSSinhvien').DataTable();

            $('#DSSinhvien tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#campaignID').val(data[0]);
                $('#participaterID').val(data[1]);
                $('#participaterName').val(data[2]);

                var birthday = new Date(data[3]);
                var strBirthday = birthday.getMonth() + '/' + birthday.getDate() + '/' + birthday.getFullYear();
                $('#participaterBirthday').val(strBirthday);

                $('#gender').val(data[4]);
                $('#participaterPhone').val(data[5]);
                $('#studentClass').val(data[6]);

                var joinday = new Date(data[7]);
                var strJoinday = joinday.getMonth() + '/' + joinday.getDate() + '/' + joinday.getFullYear();
                $('#studentPartyJoinDay').val(strJoinday);

                $('#studentMark').val(data[8]);
            });
        });

        // DS CHUC VU 
        $(document).ready(function () {
            var table = $('#DSChucvu').DataTable();

            $('#DSChucvu tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#positionID').val(data[0]);
                $('#positionName').val(data[1]);
                $('#positionSalary').val(data[2]);
                $('#positionPower').val(data[3]);
            });
        });

        // DS NHAN VIEN
        $(document).ready(function () {
            var table = $('#DSNhanvien').DataTable();

            $('#DSNhanvien tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#employeeID').val(data[0]);
                $('#employeeName').val(data[1]);
                $('#positionSelect').val(data[2]);
                $('#employeeMail').val(data[3]);
                $('#employeePhone').val(data[4]);
                $('#employeeUsername').val(data[5]);
                $('#employeePassword').val(data[6]);
            });
        });

        // DS KET QUA CONG VIEC
        $(document).ready(function () {
            var table = $('#DSKetquaCV').DataTable();

            $('#DSKetquaCV tbody').on('click', 'tr', function () {

                var data = table.row(this).data();
                $('#campaignID').val(data[0]);
                $('#jobID').val(data[1]);
                $('#participaterneed').val(data[2]);
                $('#participaterjoin').val(data[3]);
                $('#teamID').val(data[4]);
                $('#jobResult').val(data[5]);
                $('#jobPictures').val(data[6]);
            });
        });
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
